<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.oima.project.DDDProperty.model.dto.Pais" table="pais">
        <id name="idPais" type="java.lang.Long" >
            <column name="ID_PAIS" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property column="NOMBRE" name="nombre" not-null="false" type="java.lang.String">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="STATUS" name="status" not-null="false" type="java.lang.Boolean">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        
<!--        <list name="listaEstado" cascade="all" table="estado">
            <key column="ID_PAIS"/>
            <list-index column="ID_ESTADO" />
            <one-to-many class="com.oima.project.DDDProperty.model.dto.Estado"/>
        </list>-->
    </class>
</hibernate-mapping>
