<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.oima.project.DDDProperty.model.dto.CodigoPostal" table="codigo_postal">
        <id name="idCodigoPostal" type="java.lang.Long" >
            <column name="ID_CODIGO_POSTAL" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property column="NUMERO" name="numero" not-null="false" type="java.lang.Integer">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Localidad" insert="true" update="true" lazy="false" name="idLocalidad">
            <column name="ID_LOCALIDAD"/>
        </many-to-one>
        <property column="STATUS" name="status" not-null="false" type="java.lang.Byte">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
    </class>
    
</hibernate-mapping>
