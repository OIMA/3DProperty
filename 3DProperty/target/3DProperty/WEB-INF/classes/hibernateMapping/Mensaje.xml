<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.oima.project.DDDProperty.model.dto.Mensaje" table="mensaje">
        <id name="idMensaje" type="java.lang.Long" >
            <column name="ID_MENSAJE" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property column="MENSAJE" name="mensaje" not-null="false" type="java.lang.String">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="STATUS_LEIDO" name="statusLeido" not-null="false" type="java.lang.Byte">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Usuario" insert="true" update="true" lazy="false" name="idRemitente">
            <column name="ID_REMITENTE"/>
        </many-to-one>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Usuario" insert="true" update="true" lazy="false" name="idDestinatario">
            <column name="ID_DESTINATARIO"/>
        </many-to-one>
        <property column="ID_PROPIEDAD" name="idPropiedad" not-null="false" type="java.lang.Long">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="STATUS" name="status" not-null="false" type="java.lang.Byte">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
    </class>
    
</hibernate-mapping>
