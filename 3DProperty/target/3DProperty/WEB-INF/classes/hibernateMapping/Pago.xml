<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class dynamic-insert="false" dynamic-update="false" mutable="true"  name="com.oima.project.DDDProperty.model.dto.Pago" table="PAGO" optimistic-lock="version" polymorphism="implicit" select-before-update="true" >
        <id name="idPago" type="java.lang.Long" >
          <column name="ID_PAGO" not-null="true"/>
          <generator class="identity"/>
        </id>
         <property column="DESCRIPCION" name="descripcion" not-null="false" type="java.lang.String">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="MONTO" name="monto" not-null="false" type="java.lang.Double">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="STATUS" name="status" not-null="false" type="java.lang.Byte">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
    </class>
</hibernate-mapping>
