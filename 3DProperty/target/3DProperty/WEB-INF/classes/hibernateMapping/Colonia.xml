<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.oima.project.DDDProperty.model.dto.Colonia" table="colonia">
        <id name="idColonia" type="java.lang.Long" >
            <column name="ID_COLONIA" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property column="NOMBRE" name="nombre" not-null="false" type="java.lang.String">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.CodigoPostal" insert="true" update="true" lazy="false" name="idCodigoPostal">
            <column name="ID_CODIGO_POSTAL"/>
        </many-to-one>
        <property column="STATUS" name="status" not-null="false" type="java.lang.Boolean">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
    </class>
    
</hibernate-mapping>
