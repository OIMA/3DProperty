<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.oima.project.DDDProperty.model.dto.Favorito" table="favorito">
        <id name="idFavorito" type="java.lang.Long" >
            <column name="ID_FAVORITO" not-null="true"/>
            <generator class="identity"/>
        </id>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Usuario" insert="true" update="true" lazy="false" name="idUsuario">
            <column name="ID_USUARIO"/>
        </many-to-one>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Propiedad" insert="true" update="true" lazy="false" name="idPropiedad">
            <column name="ID_PROPIEDAD"/>
        </many-to-one>
        <property column="NO_ESTRELLAS" name="noEstrellas" not-null="false" type="java.lang.Byte">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
    </class>
    
</hibernate-mapping>
