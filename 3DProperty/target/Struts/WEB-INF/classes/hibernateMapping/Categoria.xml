<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.oima.project.DDDProperty.model.dto.Categoria" table="categoria">
  <id name="idCategoria" type="java.lang.Long" >
        <column name="ID_CATEGORIA" not-null="true"/>
        <generator class="identity"/>
    </id>
    <property column="NOMBRE" name="nombre" not-null="false" type="java.lang.String">
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property column="DESCRIPCION" name="descripcion" not-null="false" type="java.lang.String">
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
    <property column="STATUS" name="status" not-null="false" type="java.lang.Byte">
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
  </class>
</hibernate-mapping>
