<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.oima.project.DDDProperty.model.dto.CatalogoFotos" table="catalogo_fotos">
        <id name="idCatalogoFotos" type="java.lang.Long" >
            <column name="ID_CATALOGO_FOTOS" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property column="RUTA_FOTOGRAFIA" name="rutaFotografia" not-null="false" type="java.lang.String">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="ARCHIVO" name="archivo" type="binary">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="DESCRIPCION" name="descripcion" not-null="false" type="java.lang.String">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="VIEWPOINT" name="viewpoint" not-null="false" type="java.lang.String">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Propiedad" insert="true" update="true" lazy="false" name="idPropiedad">
            <column name="ID_PROPIEDAD"/>
        </many-to-one>
        <property column="STATUS" name="status" not-null="false" type="java.lang.Boolean">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
    </class>
</hibernate-mapping>
