<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.oima.project.DDDProperty.model.dto.RegistroPago" table="registro_pago">
        <id name="idRegistroPago" type="java.lang.Long" >
            <column name="ID_REGISTRO_PAGO" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property column="F_INICIO" name="fechaInicio" not-null="false" type="java.util.Date">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <property column="F_VENCIMIENTO" name="fechaVencimiento" not-null="false" type="java.util.Date">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Pago" insert="true" update="true" lazy="false" name="idPago">
            <column name="ID_PAGO"/>
        </many-to-one>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Contrato" insert="true" update="true" lazy="false" name="idContrato">
            <column name="ID_CONTRATO"/>
        </many-to-one>
        <property column="STATUS" name="status" not-null="false" type="java.lang.Byte">
            <meta attribute="use-in-tostring">true</meta>
            <meta attribute="use-in-equals">true</meta>
        </property>
    </class>
    
</hibernate-mapping>
