<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.oima.project.DDDProperty.model.dto.DisenioPublicidad" table="disenio_publicidad"/>
    <id name="idDisenioPublicidad" type="java.lang.Long" >
        <column name="ID_DISENIO_PUBLICIDAD" not-null="true"/>
        <generator class="identity"/>
    </id>
    <many-to-one class="com.oima.project.DDDProperty.model.dto.Disenio" insert="true" update="true" lazy="false" name="idDisenio">
        <column name="ID_DISENIO"/>
    </many-to-one>
    <many-to-one class="com.oima.project.DDDProperty.model.dto.Publicidad" insert="true" update="true" lazy="false" name="idPublicidad">
        <column name="ID_PUBLICIDAD"/>
    </many-to-one>
    <property column="STATUS" name="status" not-null="false" type="java.lang.Boolean">
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="use-in-equals">true</meta>
    </property>
</hibernate-mapping>
