<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <class name="com.oima.project.DDDProperty.model.dto.DiseniadorPropiedad" table="diseniador_propiedad">
        <id name="idDiseniadorPropiedad" type="java.lang.Long" >
            <column name="ID_DISENIADOR_PROPIEDAD" not-null="true"/>
            <generator class="identity"/>
        </id>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Usuario" insert="true" update="true" lazy="false" name="idUsuario">
            <column name="ID_USUARIO"/>
        </many-to-one>
        <many-to-one class="com.oima.project.DDDProperty.model.dto.Disenio" insert="true" update="true" lazy="false" name="idDisenio">
            <column name="ID_DISENIO"/>
        </many-to-one>
    </class>
    
</hibernate-mapping>
